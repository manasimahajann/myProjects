
public class Tester {

	public static void main(String[] args) {
		BankAccount ba = new BankAccount("Neha",25);
		printit(ba.getOwner());
		printit(ba.getBalance());
		printit("------------------");
		SavingsAccount sa = new SavingsAccount("Bob", 125);
		sa.deposit(20);
	}
	public static void printit(Object o){
		System.out.println(o);
	}
}
